{
  "name": "derhansen/sf-event-mgt-contentelements",
  "type": "typo3-cms-extension",
  "description": "Adds a relation field for content elements to events",
  "homepage": "https://github.com/derhansen/sf_event_mgt_contentelements",
  "support": {
    "issues": "https://github.com/derhansen/sf_event_mgt_contentelements/issues"
  },
  "authors": [
    {
      "name": "Torben Hansen",
      "email": "derhansen@gmail.com",
      "homepage": "https://www.derhansen.com",
      "role": "Developer"
    }
  ],
  "keywords": [
    "TYPO3 CMS",
    "event management",
    "content elements"
  ],
  "require": {
    "typo3/cms-core": "^13.4",
    "derhansen/sf_event_mgt": "^8.0.0"
  },
  "replace": {
    "typo3-ter/sf_event_mgt_contentelements": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Derhansen\\SfEventMgtContentelements\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "sf_event_mgt_contentelements"
    }
  }
}
